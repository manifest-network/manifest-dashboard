		"@sveltejs/kit": "2.48.4",
		"bignumber.js": "9.3.1",
		"carbon-icons-svelte": "13.6.0",
		"colorjs.io": "0.5.2",
		"d3": "7.9.0",
		"d3-geo": "3.1.1",
		"d3-quadtree": "3.0.1",
		"layerchart": "2.0.0-next.42",
