{
	"name": "manifest-dashboard",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "bun generate_sitemap.js && bun generate_robots.js && vite build",
		"build-release": "bun generate_sitemap.js && bun generate_robots.js && bun update-version && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"gen-sitemap": "bun generate_sitemap.js",
		"gen-robots": "bun generate_robots.js"
	},
	"resolutions": {
		"zod": "4.1.12"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "7.0.0",
		"@sveltejs/adapter-node": "5.4.0",
		"@sveltejs/vite-plugin-svelte": "6.2.1",
		"@tailwindcss/vite": "4.1.15",
		"@types/lodash": "4.17.20",
		"svelte": "5.41.1",
		"svelte-check": "4.3.3",
		"tailwindcss": "4.1.15",
		"typescript": "5.8.3",
		"vite": "7.1.11"
	},
	"dependencies": {
		"@carbon/charts-svelte": "1.27.0",
		"@skeletonlabs/skeleton": "4.1.0",
		"@skeletonlabs/skeleton-svelte": "4.1.0",
		"@sveltejs/kit": "2.47.2",
		"bignumber.js": "9.3.1",
		"carbon-icons-svelte": "13.6.0",
		"colorjs.io": "0.5.2",
		"d3": "7.9.0",
		"d3-geo": "3.1.1",
		"d3-quadtree": "3.0.1",
		"lodash": "4.17.21",
		"svelte-canvas": "2.0.2",
		"topojson-server": "3.0.1",
		"topojson-simplify": "3.0.3",
		"zod": "4.1.12"
	}
}