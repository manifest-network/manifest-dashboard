{
	"name": "manifest-dashboard",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "bun generate_sitemap.js && bun generate_robots.js && vite build",
		"build-release": "bun generate_sitemap.js && bun generate_robots.js && bun update-version && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"gen-sitemap": "bun generate_sitemap.js",
		"gen-robots": "bun generate_robots.js"
	},
	"resolutions": {
		"zod": "4.1.12"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "7.0.0",
		"@sveltejs/adapter-node": "5.4.0",
		"@sveltejs/vite-plugin-svelte": "6.2.1",
		"@tailwindcss/vite": "4.1.17",
		"@types/d3-geo": "3.1.0",
		"@types/d3-quadtree": "3.0.6",
		"@types/lodash": "4.17.20",
		"@types/topojson-client": "3.1.5",
		"@types/topojson-simplify": "3.0.3",
		"@types/topojson-specification": "1.0.5",
		"svelte": "5.43.5",
		"svelte-check": "4.3.3",
		"tailwindcss": "4.1.17",
		"typescript": "5.8.3",
		"vite": "7.2.2"
	},
	"dependencies": {
		"@fontsource/poppins": "5.2.7",
		"@fontsource/source-sans-pro": "5.2.5",
