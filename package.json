		"@tailwindcss/vite": "4.1.11",
		"@types/lodash": "4.17.19",
		"svelte": "5.34.8",
		"svelte-check": "4.2.2",
		"tailwindcss": "4.1.11",
		"typescript": "5.8.3",
		"vite": "6.2.5"
	},
	"dependencies": {
		"@carbon/charts-svelte": "1.23.13",
